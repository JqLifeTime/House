/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}


.jg20{width: 100%; height: 20rpx;background: #F5F5F5;}

.ellis-m{
	width: 100%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.pad20{
	padding: 0 20rpx;
}

.arrow{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.arrow::after{
  content: "";
  width: 12rpx;
  height: 12rpx;
  border-top: 4rpx solid #999;
  border-right: 4rpx solid #999;
  transform: rotate(135deg);
  display: block;
	margin-right: 10rpx;
	margin-top: -5rpx;
}
.arrow1{
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.arrow1::after{
  content: "";
  width: 12rpx;
  height: 12rpx;
  border-right: 4rpx solid #999;
  border-top: 4rpx solid #999;
  transform: rotate(45deg);
  display: block;
	margin-right: 10rpx;
	margin-top: -5rpx;
  margin-left: 30rpx;
}
.arrow2{
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.arrow2::after{
  content: "";
  width: 12rpx;
  height: 12rpx;
  border-bottom: 4rpx solid #999;
  border-right: 4rpx solid #999;
  transform: rotate(45deg);
  display: block;
	margin-right: 10rpx;
	margin-top: -5rpx;
  margin-left: 30rpx;
}



.fenye{
  display: flex;
  justify-content: center;
  margin: 40rpx 0;
}
.first,.last{
  width: 80rpx;
  text-align: center;
  line-height: 55rpx;
  font-size: 28rpx;
  height: 55rpx;
  border: 1px solid #ccc;
  border-radius: 10rpx;
  margin: 0 15rpx;
}
.prev,.next{
  width: 120rpx;
  text-align: center;
  line-height: 55rpx;
  font-size: 28rpx;
  height: 55rpx;
  border: 1px solid #ccc;
  border-radius: 10rpx;
}
.page1{
  font-size: 28rpx;
  line-height: 55rpx;
}
.tabActive{
  color: #e70002!important;
}
.tabActive>text{
  color: #e70002!important;
}
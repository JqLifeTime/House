.fabu{
	width: 100%;
	height: 100vh;
	display: flex;
	flex-wrap: wrap;
	justify-content: space-around;
	align-items: center;
}
.fabu>view>view{
	display: flex;
	flex-direction: column;
	justify-content: center;
	font-size: 30rpx;
	align-items: center;
	font-weight: bold;
}
.fabu view image{
	width: 120rpx;
	height: 120rpx;
	margin: 20rpx 0;
}

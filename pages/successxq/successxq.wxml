<view class="lunbo">
	<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
			<swiper-item wx:for="{{shopsimg}}">
				<image src="{{uploadimg+item}}" class="slide-image" />
			</swiper-item>
	</swiper>
</view>
<view class="shop2">
	<view class="shop2-title">
		<view class="st2-h3">{{shops.title}}({{shops.beizhu}}天转让成功)</view>
		<view class="st2-h4">更新时间：{{shops.ctime}} 浏览量：{{shops.pv}}</view>
		<view class="title-zj">
			<view class="shop2-t1">
				<view class="shop2-p">面积</view>
				<view class="shop2-h4"><text>{{shops.mianji}}㎡</text></view>
			</view>
			<view class="shop2-t1">
				<view class="shop2-p">租金</view>
				<view class="shop2-h4"><text>{{shops.price}}</text>元/月</view>
			</view>
			<view class="shop2-t1">
				<view class="shop2-p">转让费</view>
				<view class="shop2-h4"><text>{{shops.transfer}}</text>万元</view>
			</view>
		</view>
	</view>
</view>
